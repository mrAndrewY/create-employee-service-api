{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EmployeeDto",
  "type": "object",
  "required": [
    "employee_id",
    "name",
    "lastname",
    "email",
    "telephone"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "pattern": "^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,6}$"
    },
    "phone": {
      "type": "string"
    }
  }
}
